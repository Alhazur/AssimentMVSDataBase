@model List<Student>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Instrument</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            @item.Name
            @item.Phone
            @item.Location
        }
    </tbody>
</table>